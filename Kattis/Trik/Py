x = 1
for i in input():
    if i == 'A':
        if x == 1:
            x = 2
        elif x == 2:
            x = 1
        else:
            x = 3
    elif i == 'B':
        if x == 1:
            x = 1
        elif x == 2:
            x = 3
        else:
            x = 2

    else:
        if x == 1:
            x = 3
        elif x == 2:
            x = 2
        else:
            x = 1
print(x)
